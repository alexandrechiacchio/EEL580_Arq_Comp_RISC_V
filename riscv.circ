  <vhdl name="ALUControl">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY ALUControl IS
  PORT (
  ------------------------------------------------------------------------------
  --Insert input ports below
    ALUOp 	: in std_logic_vector(1 downto 0);
    func7 	: in std_logic_vector(6 downto 0);
    func3 	: in std_logic_vector(2 downto 0);

    operation 	: out std_logic_vector(3 downto 0)
    );
END ALUControl;


ARCHITECTURE TypeArchitecture OF ALUControl IS

BEGIN

process (ALUOp, func7, func3) is begin
	case ALUOp is
		when "00" =&gt; 
			operation &lt;= "0010"; --add
		when "-1" =&gt; 
			operation &lt;= "0110"; --sub
		when "1-" =&gt; 
			case func7 &amp; func3 is
				when "0000000000" =&gt;
					operation &lt;= "0010"; --add
				when "0100000000" =&gt;
					operation &lt;= "0110"; --sub
				when "0000000111" =&gt;
					operation &lt;= "0000"; --and
				when "0000000110" =&gt;
					operation &lt;= "0001"; --or
				when others =&gt;
					operation &lt;= "0000";
			end case;
		when others =&gt;
			operation &lt;= "0000";
	end case;
end process;

END TypeArchitecture;
