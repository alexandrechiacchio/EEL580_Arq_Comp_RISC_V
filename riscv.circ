LIBRARY ieee;&#13;
USE ieee.std_logic_1164.all;&#13;
&#13;
ENTITY controlModule IS&#13;
  PORT (&#13;
  ------------------------------------------------------------------------------&#13;
  --Insert input ports below&#13;
    opcode 	: in std_logic_vector(6 downto 0);

    Branch 	: out std_logic;
    MemRead 	: out std_logic;
    MemtoReg 	: out std_logic;
    ALUOp 	: out std_logic_vector(1 downto 0);
    MemWrite 	: out std_logic;
    ALUSrc	: out std_logic;
    RegWrite	: out std_logic
    );&#13;
END controlModule;&#13;


ARCHITECTURE TypeArchitecture OF controlModule IS&#13;
&#13;
BEGIN&#13;
&#13;
process (opcode) is begin
	case opcode is
		when "0110011" =&gt; -- R-type OK
			ALUSrc	&lt;= '0';
			MemtoReg 	&lt;= '0';
			RegWrite	&lt;= '1';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "10";
		when "0010011" =&gt; -- I-type + SLLI + SRLI ok?
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '0';
			RegWrite	&lt;= '1';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "10";
		when "0010111" =&gt; -- AUIPC add upperimmediate to PC ok?
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '0';
			RegWrite	&lt;= '1';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "10";
		when "0000011" =&gt; --lw OK
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '1';
			RegWrite	&lt;= '1';
			MemRead 	&lt;= '1';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "00";
		when "0110111" =&gt; --lui ok?
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '0';
			RegWrite	&lt;= '1';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "00";
		when "0100011" =&gt; --sw OK
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '-';
			RegWrite	&lt;= '0';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '1';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "00";
		when "1101111" =&gt; --jal 
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '-';
			RegWrite	&lt;= '0';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '1';
			Branch 	&lt;= '1';
			ALUOp 	&lt;= "00";
		when "1100111" =&gt; --jalr 
			ALUSrc	&lt;= '1';
			MemtoReg 	&lt;= '-';
			RegWrite	&lt;= '0';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '1';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "00";
		when "1100011" =&gt; --beq + bneq OK
			ALUSrc	&lt;= '0';
			MemtoReg 	&lt;= '-';
			RegWrite	&lt;= '0';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '1';
			ALUOp 	&lt;= "01";
		when others =&gt;
			ALUSrc	&lt;= '0';
			MemtoReg 	&lt;= '0';
			RegWrite	&lt;= '0';
			MemRead 	&lt;= '0';
			MemWrite 	&lt;= '0';
			Branch 	&lt;= '0';
			ALUOp 	&lt;= "00";
	end case;
end process;
&#13;
END TypeArchitecture;&#13;
